{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "document": true,
    "window": true,
    "it": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/anchor-is-valid": "warn",
    "react/prop-types": "off"
  }
}
